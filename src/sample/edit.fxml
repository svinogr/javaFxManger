<AnchorPane prefHeight="109.0" prefWidth="338.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.controllers.ModalEditController">
   <children>
        <TextField fx:id="textField" alignment="CENTER" layoutX="68.0" layoutY="22.0" text="Введите имя" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="25.0" />
        <Button fx:id="btnSave" defaultButton="true" layoutX="25.0" layoutY="70.0" mnemonicParsing="false" onAction="#save" prefHeight="25.0" prefWidth="150.0" text="Сохранить" AnchorPane.leftAnchor="10.0" />
        <Button fx:id="btnCancel" layoutX="248.0" layoutY="70.0" mnemonicParsing="false" onAction="#cancel" prefHeight="25.0" prefWidth="150.0" text="Отмена" AnchorPane.rightAnchor="10.0" />
   </children>
</AnchorPane>
